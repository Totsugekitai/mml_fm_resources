; チャンネル番号とトラック名の指定

A  C1  "Bass"
B  C10 "Kick/Snare"
1B C10 "HiHat"
2B C10 "Cymbal"
C  C2  "Str/brass"
1C C2  "Str/brass"
D  C11 "strings"
F  C5  "Melody"
H  C7  "piano"
1H C7  "piano"
I  C8  "melody"
Z  C9  "Exclusive"

; 拍子、テンポ、エクスクルーシブ

Z BT4,4 t180
Z EXx7e,x7f,x09,x01,xf7               r32 ; GM SYSTEM ON
Z EXx41,x10,x42,x12,{x40,0,x7f,0},xf7 r32^1-16 ; GS-RESET

