#timebase 48

; マクロの設定

$t _-1			; transpose
$i E127 BR2 BS64	; init expression & pitch bend
$k k110 J36		; kick
$s k110 J40		; snare 1
$n k110 J38		; snare 2
$h k80  J42		; closed hihat
$o k90  J44		; open hihat
$c k80  J49		; cymbal 1 (right)
$l k85  J57		; cymbal 2 (left)
